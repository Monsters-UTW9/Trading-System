<!DOCTYPE html>

<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Trading System">
    <meta name="author" content="">
    <title>@ViewBag.Title</title>
      <!--Logo-->
    <link rel="shortcut icon" href="/Images/logoIcon.ico"> 
    <!-- Bundle BootStrap Font Awesome Shared Partial Style  Site Style-->
    @Styles.Render("~/Content/SharedStyles", "~/Content/MainStyle.css")
  
</head>
<body>
    @Html.Partial("~/Views/Shared/_LanguageView.cshtml")
    <div class="form">
        <ul class="tab-group">
            <li class="tab active"><a href="#">Sign Up </a></li>
            <li class="tab"><a href="#">Log In</a></li>
        </ul>

        <div class="tab-content">

            @RenderBody()

        </div><!-- tab-content -->

    </div> <!-- /form -->
    <!--bundle JQuery BootStrap Script Theme-->
    @Scripts.Render("~/Scripts/SharedScripts", "~/Scripts/MainJs.js")
</body>
</html>
