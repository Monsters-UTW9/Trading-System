<!DOCTYPE html>

<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Trading System">
    <meta name="author" content="">
    <title>@ViewBag.Title</title>
    <!--Fonts Style-->
    <link href='http://fonts.googleapis.com/css?family=Titillium+Web:400,300,600' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/normalize/5.0.0/normalize.min.css">
    <!--Logo-->
    <link rel="shortcut icon" href="/Images/logoIcon.ico">
    <!--Site Style-->
    <link rel="stylesheet" href="/Content/MainStyle.css">
</head>
<body>
    <div class="form">
        <ul class="tab-group">
            <li class="tab active"><a href="#">Sign Up</a></li>
            <li class="tab"><a href="#">Log In</a></li>
        </ul>

        <div class="tab-content">

            @RenderBody()

        </div><!-- tab-content -->

    </div> <!-- /form -->


    <script src="@Url.Content("http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js")"></script>
    <script language="javascript" type="text/javascript" src="/Scripts/MainJs.js"></script>
</body>
</html>
